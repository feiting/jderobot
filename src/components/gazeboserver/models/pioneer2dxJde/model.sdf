        <link name="laser">
            <pose>0.22 .0 0.3 0 0 0</pose>

		  <gravity>false</gravity>
		  <inertial>
		    <mass>0.1</mass>
		  </inertial>
		  <visual name="visual">
		    <geometry>
		      <mesh>
		        <uri>model://hokuyo/meshes/hokuyo.dae</uri>
		      </mesh>
		    </geometry>
		  </visual>

		  <collision name="collision-base">
		    <pose>0 0 -0.0145 0 0 0</pose>
